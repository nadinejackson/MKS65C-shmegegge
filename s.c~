#include <stdio.h>
#include <stdlib.h>
#include <string.h>
#include <sys/types.h>
#include <sys/ipc.h>
#include <sys/shm.h>
#include <unistd.h>

int main()
{
  key_t key = ftok("./picture.jpg", 'R');
  int got = shmget(key, 200 * sizeof(char), 644 | IPC_CREAT);
  char * ptr = shmat(got, 0, 0);
  perror("ftok");
  return 0;
}
